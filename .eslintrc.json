{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "no-console":"warn",
    "import/no-extraneous-dependencies": "off",
    "require-jsdoc":"warn",
    "max-len": ["warn", {"code": 110}],
    "padded-blocks":"off"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}